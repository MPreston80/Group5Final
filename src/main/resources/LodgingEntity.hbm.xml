<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.company.entity.LodgingEntity" table="lodging" schema="qline">
        <id name="lodgingId">
            <column name="lodgingID" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(75)" length="75"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(75)" length="75"/>
        </property>
        <property name="website">
            <column name="website" sql-type="varchar(75)" length="75" not-null="true"/>
        </property>
        <property name="stationId">
            <column name="stationID" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>